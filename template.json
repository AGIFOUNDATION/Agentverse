{
	"name": "Agentverse",
	"version": "0.0.1",
	"config": {
		"process": 2,
		"port": {
			"http": 80,
			"https": 443
		},
		"certification": {
			"privateKey": "key-file-path",
			"certificate": "pem-file-path"
		},
		"page": {
			"path": "./",
			"maxage": 60000
		},
